# Redis Cache Configuration
# Enable/disable Redis caching (default: true)
REDIS_CACHE_ENABLED="true"
# Redis server host (default: localhost)
REDIS_HOST="localhost"
# Redis server port (default: 6379)
REDIS_PORT="6379"
# Redis password (optional, leave empty if no authentication)
REDIS_PASSWORD=""
# Redis database number 0-15 (default: 0)
REDIS_DB="0"
# Connection timeout in milliseconds (default: 5000)
REDIS_CONNECT_TIMEOUT="5000"
# Maximum retry attempts (default: 3)
REDIS_MAX_RETRIES="3"
# Key prefix for namespacing (default: aiom:)
REDIS_KEY_PREFIX="aiom:"
# Enable TLS/SSL (default: false)
REDIS_TLS="false"

# Redis TTL Configuration (in seconds)
# Session cache TTL (default: 3600 = 1 hour)
REDIS_TTL_SESSION="3600"
# Odoo query cache TTL (default: 300 = 5 minutes)
REDIS_TTL_ODOO="300"
# AIOM response cache TTL (default: 600 = 10 minutes)
REDIS_TTL_AIOM="600"
# Feature flag cache TTL (default: 60 = 1 minute)
REDIS_TTL_FEATURE="60"
# General purpose cache TTL (default: 300 = 5 minutes)
REDIS_TTL_GENERAL="300"
