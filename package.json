{
  "name": "automaker-starter-kit",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm run db:up && npm run db:migrate && vite dev",
    "dev:app": "vite dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "test": "playwright test tests/smoke.spec.ts --config playwright.config.ts --reporter=list",
    "test:full": "playwright test --config playwright.config.ts --reporter=list",
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/stripe/webhook"
  },
  "dependencies": {
